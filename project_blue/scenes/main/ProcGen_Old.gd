func get_scalar_field(size: Vector3, zoom: Vector3, offset: Vector3) -> Array:
	var arr : Array
	for x in range(size.x):
		for y in range(size.y):
			for z in range(size.z):
				arr[x][y][z] = noise.get_noise_3d((x+offset.x)/zoom.x,(y+offset.y)/zoom.y,(z+offset.z)/zoom.z)
	return arr

func march_cubes(scalar_field: Array):
	for x in range(scalar_field.size() - 1):
		for y in range(scalar_field[0].size() - 1):
			for z in range(scalar_field[0][0].size() - 1):
				var a : float = scalar_field[x][y][z]
				var b : float = scalar_field[x+1][y][z]
				var c : float = scalar_field[x][y][z+1]
				var d : float = scalar_field[x+1][y][z+1]
				var e : float = scalar_field[x][y+1][z]
				var f : float = scalar_field[x+1][y+1][z]
				var g : float = scalar_field[x][y+1][z+1]
				var h : float = scalar_field[x+1][y+1][z+1]
				var acc : int = 0
				acc += (1 if a < threshold else 0)
				acc += (2 if b < threshold else 0)
				acc += (4 if c < threshold else 0)
				acc += (8 if d < threshold else 0)
				acc += (16 if e < threshold else 0)
				acc += (32 if f < threshold else 0)
				acc += (64 if g < threshold else 0)
				acc += (128 if h < threshold else 0)
